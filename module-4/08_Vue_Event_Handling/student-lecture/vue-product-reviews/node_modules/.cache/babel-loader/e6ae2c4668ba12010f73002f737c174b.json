\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nexport default {\n  name: \"product-review\",\n  data: function data() {\n    return {\n      formVisiable: false,\n      newReview: {},\n      name: \"Cigar Parties for Dummies\",\n      description: \"Host and plan the perfect cigar party for all of your squirrelly friends.\",\n      reviews: [{\n        reviewer: \"Malcolm Gladwell\",\n        title: \"What a book!\",\n        review: \"It certainly is a book. I mean, I can see that. Pages kept together with glue (I hope that's glue) and there's writing on it, in some language.\",\n        rating: 3\n      }, {\n        reviewer: \"Tim Ferriss\",\n        title: \"Had a cigar party started in less than 4 hours.\",\n        review: \"It should have been called the four hour cigar party. That's amazing. I have a new idea for muse because of this.\",\n        rating: 4\n      }, {\n        reviewer: \"Ramit Sethi\",\n        title: \"What every new entrepreneurs needs. A door stop.\",\n        review: \"When I sell my courses, I'm always telling people that if a book costs less than $20, they should just buy it. If they only learn one thing from it, it was worth it. Wish I learned something from this book.\",\n        rating: 1\n      }, {\n        reviewer: \"Gary Vaynerchuk\",\n        title: \"And I thought I could write\",\n        review: \"There are a lot of good, solid tips in this book. I don't want to ruin it, but prelighting all the cigars is worth the price of admission alone.\",\n        rating: 3\n      }]\n    };\n  },\n  computed: {\n    averageRating: function averageRating() {\n      var sum = this.reviews.reduce(function (currentSum, review) {\n        return currentSum + review.rating;\n      }, 0);\n      return sum / this.reviews.length;\n    },\n    numberOfOneStarReviews: function numberOfOneStarReviews() {\n      return this.numberOfReviews(this.reviews, 1);\n    },\n    numberOfTwoStarReviews: function numberOfTwoStarReviews() {\n      return this.numberOfReviews(this.reviews, 2);\n    },\n    numberOfThreeStarReviews: function numberOfThreeStarReviews() {\n      return this.numberOfReviews(this.reviews, 3);\n    },\n    numberOfFourStarReviews: function numberOfFourStarReviews() {\n      return this.numberOfReviews(this.reviews, 4);\n    },\n    numberOfFiveStarReviews: function numberOfFiveStarReviews() {\n      return this.numberOfReviews(this.reviews, 5);\n    }\n  },\n  methods: {\n    numberOfReviews: function numberOfReviews(reviews, starType) {\n      return reviews.reduce(function (currentCount, review) {\n        return currentCount + (review.rating === starType ? 1 : 0);\n      }, 0);\n    }\n  }\n};",{"version":3,"sources":["ProductReview.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAsFA,eAAA;AACA,EAAA,IAAA,EAAA,gBADA;AAEA,EAAA,IAFA,kBAEA;AACA,WAAA;AACA,MAAA,YAAA,EAAA,KADA;AAEA,MAAA,SAAA,EAAA,EAFA;AAGA,MAAA,IAAA,EAAA,2BAHA;AAIA,MAAA,WAAA,EACA,2EALA;AAMA,MAAA,OAAA,EAAA,CACA;AACA,QAAA,QAAA,EAAA,kBADA;AAEA,QAAA,KAAA,EAAA,cAFA;AAGA,QAAA,MAAA,EACA,iJAJA;AAKA,QAAA,MAAA,EAAA;AALA,OADA,EAQA;AACA,QAAA,QAAA,EAAA,aADA;AAEA,QAAA,KAAA,EAAA,iDAFA;AAGA,QAAA,MAAA,EACA,mHAJA;AAKA,QAAA,MAAA,EAAA;AALA,OARA,EAeA;AACA,QAAA,QAAA,EAAA,aADA;AAEA,QAAA,KAAA,EAAA,kDAFA;AAGA,QAAA,MAAA,EACA,gNAJA;AAKA,QAAA,MAAA,EAAA;AALA,OAfA,EAsBA;AACA,QAAA,QAAA,EAAA,iBADA;AAEA,QAAA,KAAA,EAAA,6BAFA;AAGA,QAAA,MAAA,EACA,kJAJA;AAKA,QAAA,MAAA,EAAA;AALA,OAtBA;AANA,KAAA;AAqCA,GAxCA;AAyCA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,UAAA,GAAA,GAAA,KAAA,OAAA,CAAA,MAAA,CAAA,UAAA,UAAA,EAAA,MAAA,EAAA;AACA,eAAA,UAAA,GAAA,MAAA,CAAA,MAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA,aAAA,GAAA,GAAA,KAAA,OAAA,CAAA,MAAA;AACA,KANA;AAOA,IAAA,sBAPA,oCAOA;AACA,aAAA,KAAA,eAAA,CAAA,KAAA,OAAA,EAAA,CAAA,CAAA;AACA,KATA;AAUA,IAAA,sBAVA,oCAUA;AACA,aAAA,KAAA,eAAA,CAAA,KAAA,OAAA,EAAA,CAAA,CAAA;AACA,KAZA;AAaA,IAAA,wBAbA,sCAaA;AACA,aAAA,KAAA,eAAA,CAAA,KAAA,OAAA,EAAA,CAAA,CAAA;AACA,KAfA;AAgBA,IAAA,uBAhBA,qCAgBA;AACA,aAAA,KAAA,eAAA,CAAA,KAAA,OAAA,EAAA,CAAA,CAAA;AACA,KAlBA;AAmBA,IAAA,uBAnBA,qCAmBA;AACA,aAAA,KAAA,eAAA,CAAA,KAAA,OAAA,EAAA,CAAA,CAAA;AACA;AArBA,GAzCA;AAgEA,EAAA,OAAA,EAAA;AACA,IAAA,eADA,2BACA,OADA,EACA,QADA,EACA;AACA,aAAA,OAAA,CAAA,MAAA,CAAA,UAAA,YAAA,EAAA,MAAA,EAAA;AACA,eAAA,YAAA,IAAA,MAAA,CAAA,MAAA,KAAA,QAAA,GAAA,CAAA,GAAA,CAAA,CAAA;AACA,OAFA,EAEA,CAFA,CAAA;AAGA;AALA;AAhEA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main\">\r\n    <h2>Product Reviews for {{ name }}</h2>\r\n\r\n    <p class=\"description\">{{ description }}</p>\r\n\r\n    <div class=\"well-display\">\r\n      <div class=\"well\">\r\n        <span class=\"amount\">{{ averageRating }}</span>\r\n        Average Rating\r\n      </div>\r\n\r\n      <div class=\"well\">\r\n        <span class=\"amount\">{{ numberOfOneStarReviews }}</span>\r\n        1 Star Review{{ numberOfOneStarReviews === 1 ? '' : 's' }}\r\n      </div>\r\n\r\n      <div class=\"well\">\r\n        <span class=\"amount\">{{ numberOfTwoStarReviews }}</span>\r\n        2 Star Review{{ numberOfTwoStarReviews === 1 ? '' : 's' }}\r\n      </div>\r\n\r\n      <div class=\"well\">\r\n        <span class=\"amount\">{{ numberOfThreeStarReviews }}</span>\r\n        3 Star Review{{ numberOfThreeStarReviews === 1 ? '' : 's' }}\r\n      </div>\r\n\r\n      <div class=\"well\">\r\n        <span class=\"amount\">{{ numberOfFourStarReviews }}</span>\r\n        4 Star Review{{ numberOfFourStarReviews === 1 ? '' : 's' }}\r\n      </div>\r\n\r\n      <div class=\"well\">\r\n        <span class=\"amount\">{{ numberOfFiveStarReviews }}</span>\r\n        5 Star Review{{ numberOfFiveStarReviews === 1 ? '' : 's' }}\r\n      </div>\r\n    </div>\r\n\r\n    <a href=\"#\" v-on:click.prevent=\"formVisiable=true\" v-show=\"formVisiable===false\">Show Form</a>\r\n\r\n    <form v-show=\"formVisiable\">\r\n      <div class=\"form-element\">\r\n        <label for=\"reviewer\">Name:</label>\r\n        <input id=\"reviewer\" type=\"text\" v-model=\"newReview.reviewer\"/>\r\n      </div>\r\n      <div class=\"form-element\">\r\n        <label for=\"title\">Title:</label>\r\n        <input id=\"title\" type=\"text\" v-model=\"newReview.title\"/>\r\n      </div>\r\n      <div class=\"form-element\">\r\n        <label for=\"rating\" v-model.number=\"newReview.rating\">Rating:</label>\r\n        <select id=\"rating\">\r\n          <option value=\"1\">1 Star</option>\r\n          <option value=\"2\">2 Stars</option>\r\n          <option value=\"3\">3 Stars</option>\r\n          <option value=\"4\">4 Stars</option>\r\n          <option value=\"5\">5 Stars</option>\r\n        </select>\r\n      </div>\r\n      <div class=\"form-element\">\r\n        <label for=\"review\">Review</label>\r\n        <textarea id=\"review\" v-model=\"newReview.review\"></textarea>\r\n      </div>\r\n      <button>Submit</button>\r\n      <button type=\"cancel\" @click.prevent=\"resetForm\">Cancel</button>\r\n    </form>\r\n\r\n    <div class=\"review\" v-for=\"review in reviews\" v-bind:key=\"review.id\">\r\n      <h4>{{ review.reviewer }}</h4>\r\n      <div class=\"rating\">\r\n        <img\r\n          src=\"../assets/star.png\"\r\n          v-bind:title=\"review.rating + ' Star Review'\"\r\n          class=\"ratingStar\"\r\n          v-for=\"n in review.rating\"\r\n          v-bind:key=\"n\"\r\n        />\r\n      </div>\r\n      <h3>{{ review.title }}</h3>\r\n\r\n      <p>{{ review.review }}</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"product-review\",\r\n  data() {\r\n    return {\r\n      formVisiable: false,\r\n      newReview: {},\r\n      name: \"Cigar Parties for Dummies\",\r\n      description:\r\n        \"Host and plan the perfect cigar party for all of your squirrelly friends.\",\r\n      reviews: [\r\n        {\r\n          reviewer: \"Malcolm Gladwell\",\r\n          title: \"What a book!\",\r\n          review:\r\n            \"It certainly is a book. I mean, I can see that. Pages kept together with glue (I hope that's glue) and there's writing on it, in some language.\",\r\n          rating: 3\r\n        },\r\n        {\r\n          reviewer: \"Tim Ferriss\",\r\n          title: \"Had a cigar party started in less than 4 hours.\",\r\n          review:\r\n            \"It should have been called the four hour cigar party. That's amazing. I have a new idea for muse because of this.\",\r\n          rating: 4\r\n        },\r\n        {\r\n          reviewer: \"Ramit Sethi\",\r\n          title: \"What every new entrepreneurs needs. A door stop.\",\r\n          review:\r\n            \"When I sell my courses, I'm always telling people that if a book costs less than $20, they should just buy it. If they only learn one thing from it, it was worth it. Wish I learned something from this book.\",\r\n          rating: 1\r\n        },\r\n        {\r\n          reviewer: \"Gary Vaynerchuk\",\r\n          title: \"And I thought I could write\",\r\n          review:\r\n            \"There are a lot of good, solid tips in this book. I don't want to ruin it, but prelighting all the cigars is worth the price of admission alone.\",\r\n          rating: 3\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    averageRating() {\r\n      let sum = this.reviews.reduce((currentSum, review) => {\r\n        return currentSum + review.rating;\r\n      }, 0);\r\n      return sum / this.reviews.length;\r\n    },\r\n    numberOfOneStarReviews() {\r\n      return this.numberOfReviews(this.reviews, 1);\r\n    },\r\n    numberOfTwoStarReviews() {\r\n      return this.numberOfReviews(this.reviews, 2);\r\n    },\r\n    numberOfThreeStarReviews() {\r\n      return this.numberOfReviews(this.reviews, 3);\r\n    },\r\n    numberOfFourStarReviews() {\r\n      return this.numberOfReviews(this.reviews, 4);\r\n    },\r\n    numberOfFiveStarReviews() {\r\n      return this.numberOfReviews(this.reviews, 5);\r\n    }\r\n  },\r\n  methods: {\r\n    numberOfReviews(reviews, starType) {\r\n      return reviews.reduce((currentCount, review) => {\r\n        return currentCount + (review.rating === starType ? 1 : 0);\r\n      }, 0);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ndiv.main {\r\n  margin: 1rem 0;\r\n}\r\n\r\ndiv.main div.well-display {\r\n  display: flex;\r\n  justify-content: space-around;\r\n}\r\n\r\ndiv.main div.well-display div.well {\r\n  display: inline-block;\r\n  width: 15%;\r\n  border: 1px black solid;\r\n  border-radius: 6px;\r\n  text-align: center;\r\n  margin: 0.25rem;\r\n}\r\n\r\ndiv.main div.well-display div.well span.amount {\r\n  color: darkslategray;\r\n  display: block;\r\n  font-size: 2.5rem;\r\n  cursor: pointer;\r\n}\r\n\r\ndiv.main div.review {\r\n  border: 1px black solid;\r\n  border-radius: 6px;\r\n  padding: 1rem;\r\n  margin: 10px;\r\n}\r\n\r\ndiv.main div.review div.rating {\r\n  height: 2rem;\r\n  display: inline-block;\r\n  vertical-align: top;\r\n  margin: 0 0.5rem;\r\n}\r\n\r\ndiv.main div.review div.rating img {\r\n  height: 100%;\r\n}\r\n\r\ndiv.main div.review p {\r\n  margin: 20px;\r\n}\r\n\r\ndiv.main h3 {\r\n  display: inline-block;\r\n}\r\n\r\ndiv.main h4 {\r\n  font-size: 1rem;\r\n}\r\n\r\ndiv.main .form-element {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  padding: 0.5em;\r\n}\r\n\r\ndiv.main .form-element label {\r\n  padding: 0.5em 1em 0.5em 0;\r\n  flex: 1;\r\n}\r\n\r\ndiv.main .form-element input,\r\ndiv.main .form-element select,\r\ndiv.main .form-element textarea {\r\n  flex: 2;\r\n}\r\n\r\ndiv.main .form-element textarea {\r\n  height: 10rem;\r\n}\r\n\r\ndiv.main .form-element input,\r\ndiv.main .form-element button {\r\n  padding: 0.5em;\r\n}\r\n\r\ndiv.main .form-element button {\r\n  background: gray;\r\n  color: white;\r\n  border: 0;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/components"}]}